{
    "manifest_version": 2,
    "name": "Twitter Dopamine",
    "description": "Detoxify twitter. Hide Likes, Retweet, Followers count and notifications.",
    "version": "0.03",
    "content_scripts": [
        {
            "matches": [
                "*://twitter.com/*"
            ],
            "css": ["style.css"]

        }
    ],
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
       "128": "icon128.png" 
    }
}
